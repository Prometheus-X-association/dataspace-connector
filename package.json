        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^4.2.1",
        "inquirer": "^9.2.12",
        "json-odrl-manager": "github:Prometheus-X-association/odrl-manager#v2.0.0-alpha.5",
        "mkdirp": "^3.0.1",
        "mocha": "^10.2.0",
        "mock-fs": "^5.2.0",
        "mongodb-memory-server": "^10.0.0",
        "nodemon": "^3.0.1",
        "prettier": "^2.8.1",
        "rimraf": "^3.0.2",
